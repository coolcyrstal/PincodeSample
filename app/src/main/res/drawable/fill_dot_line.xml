<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="#FFF"/>
            <size
                android:width="5dp"
                android:height="5dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="line" >
            <stroke
                android:width="1dp"
                android:color="#000" />
            <size android:height="2dp" />
        </shape>
    </item>
</layer-list>